{
	"name": "code-ca65-syntax-color-outliner-symbols",
	"icon": "images/icon.png",
	"displayName": "ca65 Macro Assembler Language Support (6502/65816)",
	"description": "Provides language support for the ca65 macro assembler.",
	"version": "1.2.6",
	"publisher": "alberto-pilotto",
	"repository": {
		"type": "git",
		"url": "https://github.com/albertopilotto/code-ca65"
	},
	"license": "MIT",
	"engines": {
		"vscode": "^1.101.0"
	},
	"categories": [
		"Programming Languages"
	],
	"main": "./out/extension.js",
	"contributes": {
		"themes": [
			{
				"label": "CA65-ASM-Color",
				"uiTheme": "hc-black",
				"path": "./code-ca65-1-2-6/themes/CA65-ASM-Color-color-theme.json"
			}
		],
		"languages": [
			{
				"id": "ca65",
				"aliases": [
					"6502/65816 Assembly",
					"ca65"
				],
				"extensions": [
					".asm",
					".s",
					".a65",
					".inc"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "ca65",
				"scopeName": "source.ca65",
				"path": "./syntaxes/ca65.tmLanguage.json"
			}
		],
		"problemMatchers": [
			{
				"name": "ca65",
				"owner": "ca65",
				"fileLocation": "autoDetect",
				"pattern": [
					{
						"regexp": "^(.+):(\\d+):\\s+(Error|Warning|Note):\\s+(.+)$",
						"file": 1,
						"line": 2,
						"severity": 3,
						"message": 4
					}
				]
			},
			{
				"name": "ld65",
				"owner": "ca65",
				"fileLocation": "autoDetect",
				"pattern": [
					{
						"regexp": "^ld65:\\s+(Warning|Error|Note):\\s+(.+):(\\d+):\\s+(.+)$",
						"severity": 1,
						"file": 2,
						"line": 3,
						"message": 4
					}
				]
			}
		],
		"taskDefinitions": [
			{
				"type": "ca65",
				"required": [],
				"properties": {
					"config": {
						"type": "string",
						"description": "The path to the ld65 configuration file which defines the target system's memory map."
					}
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/mocha": "^10.0.1",
		"@types/node": "16.x",
		"@types/vscode": "^1.101.0",
		"@typescript-eslint/eslint-plugin": "^6.4.1",
		"@typescript-eslint/parser": "^6.4.1",
		"@vscode/test-electron": "^2.3.4",
		"eslint": "^8.47.0",
		"glob": "^10.3.3",
		"mocha": "^10.2.0",
		"typescript": "^5.1.6",
		"vscode": "^1.1.37"
	},
	"__metadata": {
		"id": "0585c3b0-418a-4d21-8fd2-cee02b99c84e",
		"publisherId": "cf169e2a-e859-4c0b-a90f-961123e43026",
		"publisherDisplayName": "Orig: Cole Campbell| Mod: Alberto Pilotto",
		"targetPlatform": "undefined",
